TABLER FORM
adding
#include <iostream>

using namespace std;

int main()
{
    int i,n;
    cout<<"Enter no";
    cin>>n;
    
     
    for(i=1;i<=10;i++)
    {
        cout<<n<<" + "<<i <<"=" <<i+n <<endl;
        
    }
   
    return 0;
}

mutipication

#include <iostream>

using namespace std;

int main()
{
    int i,n;
    cout<<"Enter no";
    cin>>n;
    
     
    for(i=1;i<=10;i++)
    {
        cout<<n<<" X "<<i <<"=" <<i*n <<endl;
        
    }
   
    return 0;
}

SUM OF N NUMBERS

#include <iostream>

using namespace std;

int main()
{
    int i,n,sum=0;
    cout<<"Enter no";
    cin>>n;
    
     
    for(i=1;i<=n;i++)
    {
        sum=sum+i;
    }
    cout<<sum<<endl;
   
    return 0;
}

FACTORIAL OF N TERMS 

#include <iostream>

using namespace std;

int main()
{
    int i,n,fac=1;
    cout<<"Enter no";
    cin>>n;
    
     
    for(i=1;i<=n;i++)
    {
        fac=fac*i;
    }
    cout<<"factorial of" <<n << "is" << fac <<endl;
   
    return 0;
}

FACTORS OF A VALUE

#include <iostream>

using namespace std;

int main()
{
    int i,n;
    cout<<"Enter no";
    cin>>n;
    
     
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            cout<<i <<endl;
        }
    }
    
   
    return 0;
}


PRIME NUMBER
#include <iostream>

using namespace std;

int main()
{
    int i,n,count=0;
    cout<<"Enter no";
    cin>>n;
    
     
    for(i=1;i<=n;i++)
    {
        if(n%i==0)
        {
            count++;;
        }
    }
    
    if(count==2)
    {
        cout<<"prime number"<<endl;
        
            
    }
        else
        {
        cout<<"not peime number"<<endl;
        }
    
    
    
   
    return 0;
}

REVERCE NUMBER

using namespace std;

int main()
{
    int r,n;
    cout<<"Enter no";
    cin>>n;
    
     
    while(n>0)
    {
        r=n%10;
        n=n/10;
        cout<<r<<endl;
    }
    
    
    
   
    return 0;
}
